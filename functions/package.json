{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "migrate:entities": "node scripts/migrateToMultipleEntities.js",
    "migrate:entities:local": "node scripts/migrateToMultipleEntitiesLocal.js",
    "test:entities": "node scripts/testMultipleEntities.js",
    "test:entities:local": "node scripts/testMultipleEntitiesLocal.js",
    "test:extraction": "node test-extraction.js"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/tasks": "^6.0.1",
    "@google-cloud/vision": "^4.3.0",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^6.3.2",
    "mercadopago": "^2.2.0",
    "twilio": "^4.23.0"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}
